				</ul>
				<i class="bi bi-list mobile-nav-toggle"></i>
			</nav><!-- .navbar -->

		</div>
	</header><!-- End Header -->
	<!-- ======= Hero Section ======= -->
	@await Html.PartialAsync("HeroSectionPartial")
